            "source": [
                "function howManyGames(p, d, m, s) {\n    let arr = [];\n    while (s > 0 && s >= p) {\n      s = s - p;\n      arr.push(\"g\");\n      if (p >= m) {\n        p = Math.max(p - d, m);\n      }\n    }\n    return arr.length;\n  }"
